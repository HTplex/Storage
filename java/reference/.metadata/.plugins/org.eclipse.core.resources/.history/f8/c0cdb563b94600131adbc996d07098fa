package multidarray;
import java.util.Scanner;
public class MultidarrayA06mutidarry {
	public static void main(String[]args){
		Scanner input=new Scanner(System.in);
		int [][][] t=new int [4][4][4];
		for(int i=0;i<t.length;i++){
			for(int i2=0;i2<t[i].length;i2++){
				for(int i3=0;i3<t[i][i2].length;i3++){
					System.out.println("please input 3d location ["+i+"]["+i2+"]["+i3+"]");
					t[i][i2][i3]=input.nextInt();
				}
			}
		}
		for(int i=0;i<t.length;i++){
			for(int i2=0;i2<t[i].length;i2++){
				for(int i3=0;i3<t[i][i2].length;i3++){
					System.out.print(t[i][i2][i3]);
				}
				System.out.println();
			}
			System.out.println();
		}
	}
}
