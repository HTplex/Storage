package oj;
import java.util.Scanner;
public class StringC05countletter {
	public static void main(String[]args){
		Scanner input=new Scanner(System.in);
		String s=input.next();
		int [] a=new int[10];
		for(int i=0;i<s.length();i++){
			for(int ii=0;ii<10;ii++){
				if(s.charAt(i)==ii)
				a[ii]++;	
			}
		}
		for(int i:a){
			System.out.println(i);
		}
	}
}
