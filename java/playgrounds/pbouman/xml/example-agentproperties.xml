<?xml version="1.0"?>
<agents xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="exampleschema.xsd">
	<agent id="agent1">
		<money utility="2" maximum="300" />
		<travel mode="pt" utility="-4" />
		<loadingTolerance maximumAccepted="0.8" utility="-8" />

		<activity name="work">
			<location x="2000.5" y="1000.25" />
			<location x="1000.5" y="1500.3" />
			<start utility="2" targetTime="8:00" acceptedDeviation="0:15" />
			<end utility="2" targetTime="17:00" acceptedDeviation="0:15" />
			<duration utility="4" targetTime="9:00" acceptedDeviation="1:00" />
		</activity>

		<home>
			<location x="1500" y="200" />
			<location x="700" y="400" />
		</home>
	</agent>
</agents>