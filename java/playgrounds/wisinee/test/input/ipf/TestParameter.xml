<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>Lyon test case</comment>
	<!--File Location-->
	<entry key="household File">./test/scenarios/ipf/HHData.csv</entry>
	<entry key="survey File">./test/scenarios/ipf/Survey.csv</entry>
	<entry key="distribution File">./test/scenarios/ipf/Pdf.csv</entry>
	<entry key="output File">./output/HHData_income.csv</entry>
	<!--Column separator of all files-->
	<entry key="spt">,</entry>	
	<!--Small value of the zero cells of the initial matrix-->
	<entry key="sValue">0.1</entry>	
	<!--Data heading for the generated column-->
	<entry key="heading">Income</entry>
	<!--Number of zones-->
	<entry key="nz">2</entry>
	<!--Identify which column in the household and survey data-->
	<!--for indicating zone where the household is located-->
	<entry key="zoneCol">2</entry>
	<!--Number of Households/Persons in the Household/Person list-->
	<entry key="lastRowData">850</entry>
	<!--Number of categories of the variable to be generated-->
	<entry key="nCol">10</entry>
	<!--Identify which column in the 'SURVEY' data for the variable to be generated-->
	<entry key="yCol">6</entry>
	<!--Number of independent variables and number of categories for each of the independent variables-->
	<entry key="nx">3</entry>
	<!--Number of categories for each of independent variables respectively-->
	<!--please use comma to separate each value-->	
	<entry key="ncx">3,5,5</entry>
	<!--Identify which column in the household and survey data for each independent variable-->
	<!--please use comma to separate each value-->
	<entry key="xCol">3,4,5</entry>
	<!--IPF calculation parameters-->
	<entry key="maxIteration">100</entry>
	<entry key="convergence">0.5</entry>	
</properties>

