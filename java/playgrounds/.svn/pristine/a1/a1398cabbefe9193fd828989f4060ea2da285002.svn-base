<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<parent>
		<groupId>org.matsim</groupId>
		<artifactId>playgrounds</artifactId>
		<version>0.7.0-SNAPSHOT</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<groupId>playground</groupId>
	<artifactId>kai</artifactId>
	<name>kai</name>

	<build>
		<defaultGoal>compiler:compile</defaultGoal>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<excludes>
					<exclude>revision.txt</exclude>
				</excludes>
			</resource>
			<resource>
				<directory>src/main/resources</directory>
				<includes>
					<include>revision.txt</include>
				</includes>
				<filtering>true</filtering>
			</resource>
		</resources>
</build>

	<repositories>
		<repository>
			<id>org.janusproject-maven</id>
			<name>Janus Project Repository</name>
			<url>http://www.janus-project.org/maven</url>
		</repository>
					<repository>
						<id>sarl-repository</id>
						<url>http://maven.sarl.io/</url>
					</repository>
	</repositories>
	<pluginRepositories>
		<pluginRepository>
			<id>org.janusproject-maven</id>
			<name>Janus Project Repository</name>
			<url>http://www.janus-project.org/maven</url>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</pluginRepository>
	</pluginRepositories>

	<dependencies>
		<dependency>
			<groupId>org.matsim.contrib</groupId>
			<artifactId>matsim4urbansim</artifactId>
			<version>0.7.0-SNAPSHOT</version>
			<scope>compile</scope>
		</dependency>
		<!--
		<dependency>
			<groupId>playground</groupId>
			<artifactId>balmermi</artifactId>
			<version>0.7.0-SNAPSHOT</version>
		</dependency>
		-->
		<dependency>
			<groupId>playground</groupId>
			<artifactId>vsp</artifactId>
			<version>0.7.0-SNAPSHOT</version>
		</dependency>
				<dependency>
			<groupId>org.janus-project.kernel</groupId>
			<artifactId>kernel</artifactId>
			<version>0.6</version>
		</dependency>

				<dependency>
					<groupId>playground</groupId>
					<artifactId>southAfrica</artifactId>
					<version>0.7.0-SNAPSHOT</version>
				</dependency>
	</dependencies>
</project>
