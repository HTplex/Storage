package error;
import java.util.*;
public class ErrorB01input {

	public static void main(String[] args) {
		Scanner input=new Scanner(System.in);
		boolean bs=true;
		do{
			try{
				int a=input.nextInt();
				int b=input.nextInt();
				bs=false;
				System.out.println(a+b);
		}
			catch(InputMismatchException e){
				System.out.println("you should input a integer");
			}
		}
		while(bs);

	}

}
